<h1>All the bands</h1>

<ul>
  <% @bands.each do |band| %>
    <form action="<%= band_url(band) %>" method="get">
      <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
      <input type="submit" value="<%= band.name %>">
    </form>
    <br>
  <% end %>
</ul>
<br>
<br>
<form action="<%= new_band_url %>" method="get">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input type="submit" value="Create New Band">
</form>
<br>
<br>
<form action="<%= user_url(current_user) %>" method="get">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input type="submit" value="Go to Your Page">
</form>
